CC = gcc
CFLAGS = -Wall -Wextra -std=c11 -g -O2
TARGET = test_hashmap
OBJS = test_hashmap.o

all: $(TARGET)

$(TARGET): $(OBJS)
	$(CC) $(CFLAGS) -o $(TARGET) $(OBJS)

test_hashmap.o: test_hashmap.c hashmap.h
	$(CC) $(CFLAGS) -c test_hashmap.c

clean:
	rm -f $(TARGET) $(OBJS)

test: $(TARGET)
	./$(TARGET)

.PHONY: all clean test

